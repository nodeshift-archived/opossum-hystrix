{
  "name": "opossum-hystrix",
  "version": "0.1.0",
  "description": "Hystrix metrics for opossum circuit breaker",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc tape test/*.js | tap-spec",
    "prerelease": "npm run test",
    "release": "standard-version -s -a",
    "lint": "standardx test/*.js index.js"
  },
  "support": {
    "target": "LTS",
    "response": "REGULAR-7",
    "backing": "COMPANY"
  },
  "keywords": [
    "circuit breaker",
    "fail fast",
    "prometheus"
  ],
  "author": "Red Hat, Inc.",
  "license": "Apache-2.0",
  "devDependencies": {
    "codacy-coverage": "^3.4.0",
    "coveralls": "^3.1.0",
    "nyc": "^15.1.0",
    "opossum": "^5.1.1",
    "standard-version": "^9.1.0",
    "standardx": "^5.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^5.1.0"
  },
  "engines": {
    "node": "^14 || ^12 || ^10"
  }
}
